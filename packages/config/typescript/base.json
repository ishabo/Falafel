{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "lib": ["dom", "es2015"],
        "baseUrl": "src/",
        "strict": true,
        "esModuleInterop": true,
        "declaration": true,
        "outDir": "dist",
        "sourceMap": true,
        "preserveSymlinks": true
    },
    "include": ["src/**/*"],
    "exclude": ["**/*.spec.ts"]
}
